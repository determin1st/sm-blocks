		display: flex
		flex-direction: row
		align-items: stretch
		justify-content: center
		> div
			font-size: var(--w3-size)
			flex: 0 0 auto
			width: auto
			height: 100%
			display: flex
			flex-direction: row
			align-items: stretch
			touch-action: none
			> div
				flex: 1 0 0
			> .goto // {{{
				max-width: 1.05em
				// previous-next
				&.PN
					// spacing for svg icon scaling
					> button
						padding: 0.25em 0 0.25em 0
						> svg
							width:  auto
							height: 100%
					// fast forward/backward state
					&.active > button
						padding: 0
				// both FL and PN are present
				&.both
					// first-next/previous-last spacing
					&.P
						padding-left:  0.05em
					&.N
						padding-right: 0.05em
			// }}}
			> .sep // {{{
				max-width: 0.19em
				padding: 0.02em 0 0.02em 0
				> svg
					fill: silver
				// left-right
				&.L
					padding-left:  0
				&.R
					padding-right: 0
			// }}}
			> .range // {{{
				////
				// configuration
				--count: 0 // number of page buttons
				--page-size: 1.34em
				// range must fit all buttons and 2 gaps,
				// minimal space that gaps may take equals to 1 page button by default
				font-size: var(--page-size)
				max-width: calc(1em + 1em * var(--count))
				////
				display: flex
				flex-direction:  row // horizontal by default
				justify-content: center // alignment (plenty of space)
				// elements are not displayed by default,
				// visibility is determined by range state
				> div
					display: none
					&.v
						display: block
				////
				> .gap
					flex: 0 100 0 // shrink only, by default
					min-width: 0.25em // exact minimal
					padding: 0.02em 0 0.02em 0
					> svg
						fill: gainsboro
				> .page
					flex: 0 0 0 // fixed (size controlled automatically)
					min-width: 1em // exact base
					padding: 0.035em 1px 0.035em 1px
					> button
						color: transparent
						font-size: var(--sm-font-size)
					// first-last pages
					&.F
						padding-left:  0
					&.L
						padding-right: 0
					// selected page
					&.x
						min-width: 1.15em
						padding-top: 0
						padding-bottom: 0
						> button
							font-size: calc(1.25 * var(--sm-font-size))
							border-color: black
							background-color: black
							color: alpha(white, 0.4)
				// range without gaps
				&.nogap
					// only page buttons
					max-width: calc(1em * var(--count))
					// dragging is not possible by default
					> .page.x > button
						cursor: default
			// }}}
		// constructed
		&.v
			> div
				font-size: var(--w3-size)
				&.flexy // {{{
					width: 100%
					> .range
						width: 100%
						max-width: 100%
						// custom spacing
						> .gap
							padding: 0.025em 0 0.025em 0
						// flexy nogap mode allows dragging,
						// page buttons are stretched
						&.nogap > .page
							flex-grow: 1
						// custom range gap when active
						&.active > .gap > svg
							fill: silver
				// }}}
				&.nosep // {{{
					> .goto
						&.P
							padding-right: 1px
						&.N
							padding-left: 1px
				// }}}
				&.v // {{{
					> .goto
						> button > svg
							fill: slategray
						&.active
							> button
								background-color: black
								> svg
									fill: white
					> .range
						> .gap > svg
							fill: silver
						> .page > button
							color: darkslategray
						> .page.x > button
							color: alpha(white, 1)
							cursor: grab
							&:hover
								color: alpha(white, 0.8)
							&:focus,
							&:active
								color: alpha(white, 1)
								box-shadow: inset 0 0 0.1em 0.1em white
						&.nogap
							> .page.x > button
								cursor: default
						&.active.drag
							cursor: grabbing
							> .page.x > button
								cursor: grabbing
								box-shadow: inset 0 0 0.1em 0.15em white
					&.flexy
						> .range > .gap > svg
							fill: gainsboro
						> .range.active > .gap > svg
							fill: silver
					&.hovered
						> .sep > svg
							fill: slategray
				// }}}
	// }}}
