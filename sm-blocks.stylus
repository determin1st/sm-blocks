		justify-content: flex-start
		pointer-events: none
		> select // {{{
			flex: 1 1 0
			font-size: var(--sm-font-size)
			padding-top:    0
			padding-bottom: 0
			padding-left:   1em
			padding-right:  var(--w3-size)
			////
			-webkit-appearance: none
			-moz-appearance: none
			appearance: none
			display: block
			////
			background-color: gainsboro
			color: transparent
			box-shadow: inset 0 0 0.6em 0 silver
			border-style: solid
			border-color: silver
			border-width: 0.025em
			border-radius: 0.05em
			outline: none
			////
			font-family: var(--sm-font-label)
			font-weight: bold
			text-align: left
			text-decoration: none
			text-overflow: ellipsis
			letter-spacing: 0
			white-space: nowrap
			&:-ms-expand
				display: none
		// }}}
		> svg // {{{
			position: absolute
			top: 0
			right: 0
			width:  1em
			height: 100%
			pointer-events: none
			fill: silver
			> .b
				fill: transparent
		// }}}
		&.a
			> select
				color: darkslateblue
			> svg
				> .a
					fill: alpha(slateblue, 0.6)
		&.v
			pointer-events: auto
			> select
				color: slategray
				cursor: pointer
				> option
					color: darkslategray
					background-color: gainsboro
					border: 1px solid red
					&:checked
						color: alpha(slateblue, 0.6)
					&:hover
						background-color: black
						color: white
			> svg
				> .a
					fill: silver
				> .b
					fill: slategray
			&.h
				> select
					color: darkslategray
					border-color: darkslategray
			&.f
				> select
					color: darkslategray
					background-color: silver
					border-color: darkslategray
					box-shadow: inset 0 0 0 0.04em white
				> svg
					> .a
						fill: slategray
					> .b
						fill: gainsboro
	// }}}
	button.sm-checkbox // {{{
		width:  100%
		height: 100%
		border-style: none
		border-width: 0
		background-color: transparent
		color: transparent
		pointer-events: none
		> svg
			display: block
			width:  100%
			height: 100%
			> .a
				fill: transparent
			> .b
				fill-rule: evenodd
				fill: slategray
			> .c
				fill-rule: evenodd
				fill: gainsboro
			> .d
				fill: transparent
			> .e
				fill: transparent
		&.x0 > svg
			// -1=intermediate (disabled)
			> .d
				fill: darkslategray
		&.x0.i > svg
			// -1=intermediate (changeable)
			> .e
				fill: darkslategray
			> .d
				fill: transparent
		&.x1 > svg
			// 0=off
		&.x2 > svg
			// 1=on
			> .c
				fill: transparent
			> .d
				fill: darkslategray
			> .e
				fill: darkslategray
		&.v
			pointer-events: auto
			&.x0
				pointer-events: none
				&.i
					pointer-events: auto
			&.h
				&.x1 > svg
					> .a
						fill: alpha(slategray, 0.3)
				&.x2 > svg
					> .d
						fill: slategray
					> .e
						fill: slategray
			&.f
				//filter: brightness(110%)
				&.x1 > svg
					> .a
						fill: alpha(slateblue, 0.3)
					> .b
						fill: slateblue
					> .c
						fill: silver
				&.x2 > svg
					> .d
						fill: slateblue
					> .e
						fill: slateblue
	// }}}
	.sm-buttons > button // {{{
		width:   100%
		height:  100%
		display: flex
		align-items: center
		justify-content: center
		////
		background-color: gainsboro
		color: slategray
		box-shadow: inset 0 0 0.4em 0 silver
		border-style: solid
		border-color: silver
		border-width: 0.05em
		border-radius: 0.1em
		////
		font-family: var(--sm-font-label)
		font-size: var(--sm-font-size)
		text-decoration: none
		letter-spacing: 0
		touch-action: none // pointer events only
		> svg
			fill: slategray
		&:hover,
		&:focus,
		&:active
			font-weight: bolder
			border-color: darkslategray
			color: darkslategray
			> svg
				fill: darkslategray
		&:focus,
		&:active
			background-color: silver
			box-shadow: inset 0 0 0 0.01em white
		// }}}
