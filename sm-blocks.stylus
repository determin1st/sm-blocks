.sm-blocks
	&.menu // {{{
		--root-size: 1em
		--item-size: 0.75em
		////
		width:  100%
		height: auto
		padding: 4px 2px 4px 2px
		overflow: visible
		> div
			width:  100%
			height: var(--root-size)
			position: relative
			overflow: visible
			opacity: 1
			////
			display: flex
			flex-direction: row
			align-items: stretch
			justify-content: flex-start
			flex-wrap: wrap
			gap: 2px
			> svg.shield // {{{
				position: absolute
				z-index:  999
				width:  0
				height: 0
				pointer-events: none
				display: none
				opacity: 0
				> polygon
					pointer-events: auto
					&.a
						cursor: default
						fill: crimson
					&.b
						cursor: pointer
						fill: slategray
				&.v
					display: block
			// }}}
			button // {{{
				width:  auto
				padding: 0.125em 0.25em 0.125em 0.25em
				position: relative
				////
				border: 0
				background-color: transparent
				box-shadow: none
				> .label // {{{
					width:  auto
					height: 100%
					align-items: center
					font-family: var(--sm-font-label)
				// }}}
				> .arrow // {{{
					position: absolute
					display:  none
					> .a
						fill: rgba(var(--sm-color-5))
					> .b
						fill: rgba(var(--sm-color-2))
				// }}}
				&.drop > .arrow
					display: block
			// }}}
			> button // {{{
				width:  auto
				height: var(--root-size)
				opacity: 0.6
				flex: 0 0 auto
				> .label // {{{
					width:  auto
					height: 100%
					justify-content: center
					font-size: calc(var(--root-size) / 2) // 24px
				// }}}
				> .arrow // {{{
					width:  calc(0.75 * var(--root-size))
					height: calc(0.75 * var(--root-size))
					top:    calc(0.125 * var(--root-size))
					right:  0
					transform: rotate(90deg)
					> .a
						fill: rgba(var(--sm-color-5))
					> .b
						fill: rgba(var(--sm-color-2))
				// }}}
				> .line // {{{
					width:  100%
					height: calc(0.084 * var(--root-size)) // 4px
					position: absolute
					left: 0
					display: flex
					flex-direction: row
					opacity: 0
					> svg
						height: 100%
						//fill: transparent
						&.a
							flex: 0 0 0
							min-width: calc(0.125 * var(--root-size))
							//width: 0.167em
						&.b
							flex: 1 1 0
						> .a
							fill: rgba(var(--sm-color-4))
						> .b
							fill: rgba(var(--sm-color-2))
					// variants
					&.a
						top: 0
					&.b
						bottom: 0
				// }}}
				// has dropdown
				&.drop
					padding-right: calc(0.75 * var(--root-size))
				// hovered, focused
				&.v.h,
				&.v.f
					> .line
						opacity: 1
				&.v.f
					box-shadow: inset 0 0 6px -2px rgba(var(--sm-color-7))
				// opened
				&.v.o
					box-shadow: inset 0 0 6px -2px rgba(var(--sm-color-3))
					> .arrow
						transform: rotate(0deg)
						> .a
							fill: rgba(var(--sm-color-4))
						> .b
							fill: rgba(var(--sm-color-3))
					> .line
						opacity: 1
					&.f
						background-color: rgba(var(--sm-color-7), 0.025)
						> .arrow > .a
							fill: rgba(var(--sm-color-6))
				// clicked (waiting)
				&.v.w,
				&.v.w.h,
				&.v.w.f
					opacity: 1
					background-color: rgba(var(--sm-color-0))
					> .label
						color: rgba(var(--sm-color-1))
					> .line
						//height: 0.125em // 6px
						height: 0.167em // 8px
						opacity: 1
						> .a > .a,
						> .b > .a
							fill: rgba(var(--sm-color-1))
				// current
				&.x
					opacity: 1
					background-color: rgba(var(--sm-color-3))
					//> .label
					//	color: rgba(var(--sm-color-0))
					> .line
						height: 0.167em // 8px
						opacity: 1
						> .a > .a,
						> .b > .a
							fill: rgba(var(--sm-color-1))
						> .a > .b,
						> .b > .b
							fill: rgba(var(--sm-color-0))
			// }}}
			.dropdown // {{{
				padding: 0.333em 0.167em // 16px 8px
